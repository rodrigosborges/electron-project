{
  "name": "electron-angularjs-sqlite",
  "version": "1.0.0",
  "description": "Um aplicativo simples usando AngularJS, Sqlite, Electron e Bootstrap",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "win64": "electron-packager . myApp --platform=win32 --arch=x64",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "win": "electron-builder --win --x64 --dir"
  },
  "devDependencies": {
    "angular": "^1.5.1",
    "angular-route": "^1.5.1",
    "angular-utils-pagination": "^0.11.0",
    "electron-builder": "^20.26.1",
    "electron-builder-squirrel-windows": "^20.27.0",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.36.0",
    "electron-rebuild": "^1.8.2",
    "jquery": "^2.2.2",
    "sqlite-sync": "^0.2.1"
  },
  "build": {
    "appId": "your.id"
  }
}
